#ifndef CONFEDERACION_H_
#define CONFEDERACION_H_
#include <stdio.h>
#include <stdlib.h>
#include <string.h>


typedef struct{
	int id;
	char nombre[50];
	char region[50];
	int anioCreacion;
	short isEmpty;

}Confederacion;





#endif /* CONFEDERACION_H_ */
